{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100,
      "MaxRequestBodySize": 104857600
    },
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:2291"
      }
    }
  },
  // If you set these variables then you dont use local storage for backend settings.
  // If you leave them empty as below, then you can set your backend settings in the UI using localStorage
  "AppSettings": {
    "BaseUrl": "",
    "Password": ""
  }
}
